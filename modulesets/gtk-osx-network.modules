    <branch module="libtasn1/libtasn1-4.19.0.tar.gz"
            version="4.19.0"
            hash="sha256:1613f0ac1cf484d6ec0ce3b8c06d56263cc7242f1c23b30d82d23de345a63f7a"
            repo="ftp.gnu.org">
    </branch>
  </autotools>
  <!---->
  <cmake id="zlib">
    <branch module="libpng/zlib-1.2.13.tar.xz"
