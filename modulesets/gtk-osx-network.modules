  </meson>
  <!---->
  <autotools id="libpsl"
             autogen-sh='configure'>
    <branch module="rockdaboot/libpsl"
            repo="github"
            revision="libpsl-0.21.2" />
  </autotools>
  <!---->
  <meson id="libsoup"
         mesonargs="-Dvapi=disabled">
    <branch revision="libsoup-2-74" />
    <dependencies>
      <dep package="libpsl" />
      <dep package="python3" />
      <dep package="vala" />
      <dep package="glib" />
      <dep package="glib-networking" />
