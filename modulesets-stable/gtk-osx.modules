    </branch>
    <dependencies>
      <dep package="glib"/>
      <dep package="cairo"/>
    </dependencies>
      <after>
        <dep package='python'/>
        <dep package='python3'/>
      </after>
  </autotools>

  <autotools id="fribidi" autogenargs="--disable-docs">
    <branch module="fribidi/fribidi/releases/download/v1.0.5/fribidi-1.0.5.tar.bz2"
            repo="github-tarball" version="1.0.5"/>
    <dependencies>
      <!--dep package="c2man"/ -->
    </dependencies>
  </autotools>

